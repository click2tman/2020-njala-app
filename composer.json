{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.7",
        "drupal/admin_toolbar": "^1.27",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/adminimal_theme": "^1.5",
        "drupal/advagg": "^4.0",
        "drupal/antibot": "^1.2",
        "drupal/autologout": "^1.0",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/blazy": "^2.0@RC",
        "drupal/block_class": "^1.0",
        "drupal/cache_control_override": "^1.0@alpha",
        "drupal/captcha": "^1.0@beta",
        "drupal/cdn": "^3.3",
        "drupal/clamav": "^1.1",
        "drupal/claro": "^1.0@alpha",
        "drupal/clientside_validation": "^1.2",
        "drupal/cloudfront_purger": "^1.1",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.7.0",
        "drupal/crop": "^1.5",
        "drupal/drupalchat": "^1.0",
        "drupal/duration_field": "^1.6",
        "drupal/entity_print": "^2.1",
        "drupal/entity_reference_exposed_filters": "^1.0",
        "drupal/facets": "^1.4",
        "drupal/field_group": "^3.0",
        "drupal/flippy": "^1.0@beta",
        "drupal/fontawesome": "^2.13",
        "drupal/fontyourface": "^3.2",
        "drupal/ga_webform": "^1.0@alpha",
        "drupal/honeypot": "^1.29",
        "drupal/imageapi_optimize": "^2.0@alpha",
        "drupal/imce": "^1.7",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/lightning_layout": "^2.0",
        "drupal/lightning_workflow": "^3.6",
        "drupal/login_security": "^1.5",
        "drupal/maestro": "^2.1@RC",
        "drupal/memcache": "^2.0",
        "drupal/memcache_storage": "1.x-dev",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/message": "^1.0",
        "drupal/nodejs": "^1.2",
        "drupal/onesignal": "1.x-dev",
        "drupal/paragraphs": "^1.8",
        "drupal/password_policy": "^3.0@alpha",
        "drupal/printfriendly": "^3.4",
        "drupal/private_message": "^1.2",
        "drupal/private_message_queue": "^1.6",
        "drupal/profile": "^1.0@RC",
        "drupal/purge": "^3.0@beta",
        "drupal/purge_queuer_url": "^1.0@RC",
        "drupal/realname": "^1.0@RC",
        "drupal/recaptcha": "^2.4",
        "drupal/redirect": "^1.3",
        "drupal/reg_confirm_email": "^1.0",
        "drupal/registration_validation": "^1.2",
        "drupal/rename_admin_paths": "^1.1",
        "drupal/rocket_chat": "^1.1",
        "drupal/rules_data_exchanger": "^1.0",
        "drupal/rules_token": "^1.1",
        "drupal/rules_user_fields": "^1.1",
        "drupal/rules_webform": "^1.4",
        "drupal/schema_metatag": "^1.4",
        "drupal/seckit": "^1.2",
        "drupal/smtp": "^1.0@beta",
        "drupal/superfish": "^1.3",
        "drupal/taxonomy_manager": "^1.0@alpha",
        "drupal/telephone_validation": "^2.2",
        "drupal/token": "^1.5",
        "drupal/tokenuuid": "^1.2",
        "drupal/tvi": "^1.0@beta",
        "drupal/username_enumeration_prevention": "^1.0",
        "drupal/username_policy": "^1.0",
        "drupal/validators": "^1.1",
        "drupal/video_embed_field": "^2.1",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/webform": "^5.3",
        "drupal/webform_analysis": "^1.0@beta",
        "drupal/webform_confirmation_file": "^1.0@alpha",
        "drupal/webform_digests": "^1.0@alpha",
        "drupal/webform_encrypt": "^1.0@alpha",
        "drupal/webform_extra_field": "^1.0@alpha",
        "drupal/webform_invitation": "^1.0",
        "drupal/webform_ip_track": "^1.0",
        "drupal/webform_mass_email": "^1.0@beta",
        "drupal/webform_prepopulate": "^1.0@alpha",
        "drupal/webform_rest": "^2.0@beta",
        "drupal/webform_submission_files_download": "^1.0@alpha",
        "drupal/webform_user_registration": "1.x-dev",
        "drupal/webform_views": "^5.0@alpha",
        "drupal/webform_wizard_full_title": "^1.3",
        "drupal/webp": "^1.0@beta",
        "drupal/wkhtmltopdf": "^1.2",
        "drupal/yoast_seo": "^1.5",
        "drush/drush": "^9.0.0",
        "mikehaertl/phpwkhtmltopdf": "~2.1",
        "tecnickcom/tcpdf": "~6",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "^1.4",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
